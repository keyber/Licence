#include "pgm.h"

void histogramme (int his [256], unsigned char *buf, long size){
	int i;

	for (i = 0; i < 256; ++i) {
		his[i] = 0;;
	}

	for (i = 0; i < size; ++i) {
		his[buf[i]]++;
	}
}

int main(int argc, char **argv) {
	if(argc!=2){
		printf("1 arg requis");
		return 1;
	}

	int w, h;
	char* buf = readpgm(argv[1], w, h);
	return 0;
}
